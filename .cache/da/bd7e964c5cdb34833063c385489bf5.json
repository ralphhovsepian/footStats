 {\r\n    background-color: #f7f7f7 !important;\r\n    font-family: 'Roboto', sans-serif;\r\n    color: #333;\r\n    overflow-x: hidden;\r\n}\r\n\r\n\r\n#nav-matches {\r\n    @include navbar(whitesmoke, 50px, row);\r\n    margin-top: 55px;\r\n    z-index: 1;\r\n}\r\n\r\n#gamesInfo {\r\n    margin-top: 10px;\r\n    display: flex;\r\n    flex-direction: column;\r\n    flex-wrap: wrap;\r\n    .card {\r\n        margin: 20px 0;\r\n        border-left: 4px solid #5bc0de;\r\n    }\r\n    p {\r\n        font-weight: bold;;\r\n    }\r\n}\r\n\r\n#leagueOption {\r\n    margin-top: 150px;\r\n}\r\n\r\n#selectLeague {\r\n    display: none;\r\n}\r\n\r\n#homeInfo {\r\n    text-align: center;\r\n    img {\r\n        @include size(200px, 200px);\r\n    }\r\n}\r\n\r\n\r\n#teams {\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: space-around;\r\n    flex-wrap: wrap;\r\n    .team {\r\n        background-color: #fff;\r\n        border: 1px solid #ccc;\r\n        border-bottom: 6px solid #5bc0de;\r\n        border-radius: 6px;\r\n        opacity: .9;\r\n        padding: 20px;\r\n        margin: 30px;\r\n        cursor: pointer;\r\n    }\r\n    img {\r\n        @include size(100px, 100px);\r\n    }\r\n}\r\n\r\n#teamInfo {\r\n    display: none;\r\n    flex-direction: row;\r\n    justify-content: space-between;\r\n    flex-wrap: wrap;\r\n    margin-left: 30px;\r\n    img {\r\n        @include size(150px, 150px);\r\n        margin: 35px 0;\r\n    }\r\n}\r\n\r\n#teamSchedule {\r\n    margin: 0 auto;\r\n    width: 50%;\r\n}\r\n\r\n@media only screen and (min-width: 575px) {\r\n    #gamesInfo, #leagueOption {\r\n        margin-left: 20px;\r\n    }\r\n}\r\n\r\n@media only screen and (max-width: 575px) {\r\n   nav {\r\n       button {\r\n           display: none !important;\r\n       }\r\n   }\r\n}"},"lineCount":null}},"error":null,"hash":"ca0d138645d67977c8c6b73f309e9849","cacheData":{"env":{}}}